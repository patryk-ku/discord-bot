{
  "name": "discord-bot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon index.js",
    "start": "npx pm2 start index.js --name 'discord-bot' --log 'bot.log'",
    "restart": "npx pm2 restart 'discord-bot'",
    "stop": "npx pm2 delete 'discord-bot' && npx pm2 kill",
    "list": "npx pm2 ls",
    "monit": "npx pm2 monit",
    "logs": "npx pm2 logs 'discord-bot'",
    "deploy-commands": "node ./scripts/deploy-commands.js",
    "deploy-global-commands": "node ./scripts/deploy-global-commands.js",
    "reset-commands": "node ./scripts/reset-commands.js",
    "termux-debug": "nohup node index.js >> ../output.log"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@discordjs/voice": "^0.16.1",
    "@napi-rs/canvas": "^0.1.44",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "libsodium-wrappers": "^0.7.13",
    "opusscript": "^0.0.8",
    "pm2": "^5.3.0",
    "sequelize": "^6.35.0",
    "sqlite3": "^5.1.6",
    "validator": "^13.11.0",
    "ytsr": "^3.8.4"
  },
  "devDependencies": {
    "eslint": "^8.53.0",
    "nodemon": "^3.0.1"
  }
}
